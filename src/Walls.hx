	static function reflect_horizontal(a: Array<Array<Int>>): Array<Array<Int>> {
		var out = new Array<Array<Int>>();
		for (i in 0...a.length) {
			out.push(new Array<Int>());
			for (j in 0...a[0].length) {
				out[i].push(a[i][a[i].length - 1 - j]);
			}
		}
		return out;
	}

	static function reflect_vertical(a: Array<Array<Int>>): Array<Array<Int>> {
		var out = new Array<Array<Int>>();
		for (i in 0...a.length) {
			out.push(new Array<Int>());
			for (j in 0...a[0].length) {
				out[i].push(a[a.length - 1 - i][j]);
			}
		}
		return out;
	}

	static function same(a: Array<Array<Int>>, b: Array<Array<Int>>): Bool {
		for (i in 0...a.length) {
			for (j in 0...a[0].length) {
				if (a[i][j] != b[i][j]) {
					return false;
				}
			}
		}
		return true;
	}

